{
  "extends": ["airbnb-base", "prettier"],
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-underscore-dangle": 1,
    "consistent-return": "warn",
    "prettier/prettier": [
      "error",
      {
        "bracketSpacing": false,
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "semi": true
      }
    ]
  },
  "plugins": ["prettier"]
}
